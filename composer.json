{
    "name" : "thelia/thelia",
    "description" : "Thelia is an ecommerce CMS.",
    "license" : "LGPL-3.0+",
    "homepage" : "http://thelia.net/v2",
    "support" : {
        "forum" : "http://thelia.net/forum",
        "wiki" : "http://doc.thelia.net"
    },
    "require":{
        "php": ">=5.4",
        "ircmaxell/password-compat": "1.0.*",
        "propel/propel": "dev-master",
        "psr/log" : "1.0",
        "symfony/class-loader": "2.2.*",
        "symfony/config" : "2.2.*",
        "symfony/console" : "2.2.*",
        "symfony/dependency-injection" : "2.2.*",
        "symfony/event-dispatcher" : "2.2.*",
        "symfony/http-kernel" : "2.2.*",
        "symfony/routing" : "2.2.*",
        "symfony/filesystem" : "2.2.*",
        "symfony/yaml" : "2.2.*",
        "symfony/translation" : "2.2.*",

        "symfony-cmf/routing": "1.0.0",

        "symfony/form": "2.2.*",
        "symfony/validator": "2.3.*",

        "smarty/smarty": "v3.1.14",
        "kriswallsmith/assetic": "1.2.*@dev",
        "leafo/lessphp": "0.4.*",
        "ptachoire/cssembed": "1.0.*",

        "simplepie/simplepie": "dev-master",

        "imagine/imagine": "0.*",
        "symfony/icu": "1.0",
        "swiftmailer/swiftmailer": "5.0.*",
        "symfony/serializer": "2.3.*",
        "ensepar/html2pdf": "1.0.1",
        "symfony/finder": "~2.2"
    },
    "require-dev" : {
        "phpunit/phpunit": "3.7.*",
        "fzaninotto/faker": "dev-master",
        "maximebf/debugbar": "dev-master"
    },
    "minimum-stability": "stable",
    "config" : {
        "vendor-dir" : "core/vendor",
        "bin-dir" : "bin"
    },
    "autoload": {
        "psr-0": {
            "": "local/modules/",
            "Thelia" : "core/lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "smarty/smarty",
                "version": "3.1.14",
                "dist": {
                    "url": "http://www.smarty.net/files/Smarty-3.1.14.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "http://smarty-php.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "tags/Smarty_3_1_14/distribution/"
                },
                "autoload": {
                    "classmap": ["libs/"]
                }
            }
        }
    ]
}
