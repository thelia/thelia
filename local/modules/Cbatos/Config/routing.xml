<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

	<route id="Cbatos.answer" path="/cbatos/answer" methods="post">
        <default key="_controller">Cbatos\Controller\CbatosControllerAnswer::resp</default>
    	</route>

	<route id="Cbatos.manuel" path="/cbatos/manuel" methods="post">
	<default key="_controller">Cbatos\Controller\CbatosControllerBack::manu</default>
	</route>

  <route id="cbatos.saveconfig" path="/admin/module/Cbatos/saveconfig" methods="post">
        <default key="_controller">Cbatos\Controller\CbatosAdminSave::save</default>
    </route>

</routes>
